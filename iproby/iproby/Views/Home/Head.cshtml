<script type="text/javascript">
    $(document).ready(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                $('.scrollup_btn').fadeIn();
            } else {
                $('.scrollup_btn').fadeOut();
            }
        });
        $('.scrollup_btn').click(function () {
            $("html, body").animate({ scrollTop: 0 }, 600);
            return false;
        });
    });
</script>
