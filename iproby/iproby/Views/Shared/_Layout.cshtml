<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/Scripts/jquery-1.9.1.min.js" type="text/javascript"></script>
    <link href="~/Content/bootstrap/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/skeleton.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/Validator.js"></script>
    <script src="~/Scripts/jquery.mask.min.js"></script>
    <style>

    </style>
</head>
<body>
    <div class="container">
@if (IsSectionDefined("Registration"))
    {
        @RenderSection("Registration")
    }
@if (IsSectionDefined("Login"))
{
    @RenderSection("Login")
}
    <div class="row header_top">
        <div class="col-md-1"></div>
        <div class="col-md-1"><img src="~/Content/img/ipro_header_top.png" /></div>
        <div class="col-md-1">ГЛАВНАЯ</div>
        <div class="col-md-1">О НАС</div>
        <div class="col-md-1">КОНТАКТЫ</div>
        <div class="col-md-1">ФОРУМ</div>
        <div class="col-md-1"></div>
        @*<div class="col-md-1"></div>*@
            <div class="col-md-1"></div>
            <div class="col-md-1 enter">ВОЙТИ</div>
            <div class="col-md-2 registration" css="custom">РЕГИСТРАЦИЯ</div>
            <div class="col-md-1"></div>
        </div>
        <div class="row header_middle" style="background-color: #ececec; padding: 10px 10px 10px 10px; height: 530px; background: -webkit-linear-gradient(white, #ececec); /* For Safari 5.1 to 6.0 */ background: -o-linear-gradient(white, #ececec); /* For Opera 11.1 to 12.0 */ background: -moz-linear-gradient(white, #ececec); /* For Firefox 3.6 to 15 */ background: linear-gradient(white, #ececec); /* Standard syntax */ ">
            <div class="col-md-6" style="  padding-left: 0; padding-right: 0;">
                <img src="~/Content/img/header_middle_main_left.png" />
            </div>

            <div class="col-md-6" style=" padding-left: 0; padding-right: 0;">
                <img src="~/Content/img/header_middle_main_right.png" style="margin-left:90px;" />
            </div>

        </div>
    </div>


    @RenderBody()
    <script src="~/Scripts/main.js"></script>
   
    @RenderSection("scripts", required: false)


</body>
</html>
